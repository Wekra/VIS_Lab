<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <constant name="struts.devMode" value="false" />
    <constant name="struts.custom.i18n.resources" value="ApplicationResources" />




    <!-- Add packages here -->

    <package name="vislabExample" extends="struts-default">

        <!-- Add your actions here  -->

        <action name="User_forward">
            <result>/WEB-INF/example/Login.jsp</result>
        </action>

        <action name="User_login" class="vislabExample.controller.action.LoginAction">
            <result name="success">/WEB-INF/example/welcome.jsp</result>
            <result name="input">/WEB-INF/example/Login.jsp</result>
            <result name="registrieren">/WEB-INF/example/registration.jsp</result>
        </action>
    </package>


</struts>
